<UserControl x:Class="ChronClient.Controls.Themes.Matrix.ArrayListInstance"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChronClient.Controls"
             mc:Ignorable="d" 
             Margin="0,-1,0,0"
             d:DesignHeight="20" d:DesignWidth="300">
    <Grid>
        <Grid x:Name="AnimatedGrid" Height="20">
            <TextBlock x:Name="Text" Padding="5,0,4,0" Margin="0,0,0,0" HorizontalAlignment="Right" FontFamily="/ChronClient;component/res/fonts/#Mojangles" FontSize="16" Background="Black" Foreground="{StaticResource ChronClient_Theme_Matrix_Accent}"
                       Height="auto"  Text="MODULE (Setting) [Keybind]" VerticalAlignment="Bottom" RenderOptions.EdgeMode="Aliased"/>
        </Grid>
    </Grid>
</UserControl>
